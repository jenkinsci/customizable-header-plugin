<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler">
    <st:include page="logo" it="${it.logo}"/>
    <j:if test="${it.logoText != null and !(it.logoText eq '')}">
        <div class="custom-header__text">${it.logoText}</div>
    </j:if>
    <j:if test="${it.title != null and !(it.logoText eq '')}">
        <div class="custom-header__text">${it.title}</div>
    </j:if>
</j:jelly>
